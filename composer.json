{
    "name": "repat/willdurand-nmap",
    "description": "nmap is a PHP wrapper for Nmap, a free security scanner for network exploration. Updated for PHP 7.1",
    "license": "MIT",
    "version": "0.8.2",
    "authors": [
        {
            "name": "William DURAND",
            "email": "william.durand1@gmail.com"
        },
        {
            "name": "repat",
            "email": "repat@repat.de"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "symfony/process": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
      "psr-4": {
          "Nmap\\": "src/Nmap/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Nmap\\Test\\": "tests/NMap/Tests"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
